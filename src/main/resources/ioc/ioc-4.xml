<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	
	<bean id="cpu" class="java.lang.String">
		<constructor-arg value="I5"></constructor-arg>
	</bean>
	<bean id="ram" class="java.lang.String">
		<constructor-arg value="16G"></constructor-arg>
	</bean>
	<bean id="hdd" class="java.lang.String">
		<constructor-arg value="1TB"></constructor-arg>
	</bean>
	<context:component-scan base-package="com.sp.bdi.test4"></context:component-scan>
</beans>
